{
  "name": "spryker/product-category-search",
  "type": "library",
  "description": "ProductCategorySearch module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/category": "^6.0.0",
    "spryker/kernel": "^3.49.0",
    "spryker/product-category": "^5.0.0",
    "spryker/product-page-search-extension": "^1.3.0",
    "spryker/propel-orm": "^1.5.0",
    "spryker/store": "^1.4.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/product-page-search": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/product-page-search": "If you want to use ProductPageSearch constants."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
